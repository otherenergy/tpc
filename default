server {
    listen 8080;
    listen [::]:8080;
    server_name smartcret.es;

    location ~ /\.env {
        deny all;
    }

    return 301 $scheme://www.smartcret.es$request_uri;
}

# ------------CONFIG PROD-----------
server {
    listen 8080;
    listen [::]:8080;
    root /home/site/wwwroot/prod;
    index index.php index.html index.htm;
    server_name www.smartcret.es; 
    port_in_redirect off;

    location / {
        try_files $uri $uri/ @rewriter;

        if (!-e $request_filename) {
            rewrite ^(.*)$ $1.php last;
        }
    }

    location ~ /\.env {
        deny all;
    }

    location @rewriter {
        rewrite ^(.*[^/])$ $1.php last;

        try_files $uri =404;
    }

    #-----------404--------------
    location @handle404 {
        set $lang "";
        
        if ($request_uri ~* ^/es/) {
            set $lang "es";
        }

        if ($request_uri ~* ^/en-us/) {
            set $lang "en-us";
        }

        if ($request_uri ~* ^/en-gb/) {
            set $lang "en-gb";
        }

        if ($request_uri ~* ^/it/) {
            set $lang "it";
        }

        if ($request_uri ~* ^/fr/) {
            set $lang "fr";
        }

        if ($request_uri ~* ^/de/) {
            set $lang "de";
        }


        if ($lang = "") {
            # Si no se detecta ningún idioma, puedes usar un 404 predeterminado o redirigir a un idioma predeterminado.
            return 404;
        }

        return 301 https://www.smartcret.es/$lang/404;
    }

    error_page 404 = @handle404;

    #----------REDIRECCIONES---------
